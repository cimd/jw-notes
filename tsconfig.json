{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "paths": {
      "modules/*": [
        "src/modules/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "boot/*": [
        "src/boot/*"
      ]
    },
    "baseUrl": "."
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}
