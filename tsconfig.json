{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "paths": {
      "modules/*": [
        "src/modules/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "boot/*": [
        "src/boot/*"
      ]
    },
    "baseUrl": ".",
    "sourceMap": false,
    "types": ["cypress", "node", "cypress-real-events", "@types/lodash", "cypress-terminal-report"],
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
    ],
  }
}
